project AMQP is

   for Languages use ("Ada");
   for Source_Dirs use ("src", "test");
   for Object_Dir use "obj";
   for Exec_Dir use "bin";
   for Main use ("test/amqp_test.adb", "test/connection_test.adb");

   package Compiler is
      for Default_Switches ("Ada") use ("-gnat2012", "-gnatwa", "-gnatwe", "-O2");
   end Compiler;

   package Binder is
      for Default_Switches ("Ada") use ("-Es");
   end Binder;

end AMQP;
