project AMQP is

   for Languages use ("Ada");
   for Source_Dirs use ("src", "test");
   for Object_Dir use "obj";
   for Exec_Dir use "test";
   for Main use ("amqp_test.adb", "connection_test.adb", "handshake_test.adb", "channel_test.adb", "pubsub_test.adb", "url_test.adb");

   package Compiler is
      for Default_Switches ("Ada") use ("-gnat2012", "-gnatwa", "-gnatwe", "-O2");
   end Compiler;

   package Binder is
      for Default_Switches ("Ada") use ("-Es");
   end Binder;

end AMQP;
