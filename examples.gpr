with "amqp.gpr";

project Examples is
   for Source_Dirs use ("examples");
   for Object_Dir use "obj";
   for Exec_Dir use "examples";
   for Main use ("publisher.adb", "subscriber.adb");

   package Compiler is
      for Default_Switches ("Ada") use ("-gnat2012", "-gnatwa", "-O2");
   end Compiler;

   package Binder renames AMQP.Binder;
end Examples;
